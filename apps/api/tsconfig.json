{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "outDir": "./dist",
    "baseUrl": "./"
  }
}
